{
  "name": "vite-plugin-prql",
  "description": "Vite plugin to pre-processes PRQL queries into SQL queries at build time.",
  "version": "0.8.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaryHal/vite-plugin-prql"
  },
  "keywords": [
    "vite",
    "prql"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/utils.d.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly && esbuild src/index.ts src/utils.ts src/types.ts --minify --sourcemap --outdir=dist",
    "test": "vitest",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "esbuild": "^0.17.11",
    "prql-js": "^0.6",
    "rimraf": "^4.3.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "peerDependencies": {
    "prql-js": "*",
    "vite": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "magic-string": "^0.30.0",
    "parse-literals": "^1.2.1",
    "tosource": "2.0.0-alpha.3"
  }
}
